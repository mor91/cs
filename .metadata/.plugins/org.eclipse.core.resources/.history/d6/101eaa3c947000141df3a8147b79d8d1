#ifndef ROAD_H
#define	ROAD_H
#include <string>

class Road {
    std::string _startingJunction;
    std::string _endJunction;
    int _length;
public:
    Road();
    Road(const std::string &startingJunction, const std::string &endJunction, int length);
    virtual ~Road();

    

};

#endif	/* ROAD_H */

