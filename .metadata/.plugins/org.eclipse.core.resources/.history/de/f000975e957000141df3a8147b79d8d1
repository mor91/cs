#ifndef CAR_H
#define	CAR_H
#include <string>

class Car {
    std::string _carID;
    std::string _roadPlan;
    bool _goodCondition;
    int _distanceFromBeginningJunction;
public:

    Car(const std::string carID,const std::string roadPlan);
    virtual ~Car();
    

};

#endif	/* CAR_H */


